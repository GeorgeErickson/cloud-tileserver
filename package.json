{
  "name": "aws-tileserver",
  "version": "1.0.0",
  "description": "AWS lambda function to handle vectortile queries via REST",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "sim": "tsc && PGUSER=postgres PGHOST=127.0.0.1 PGDATABASE=slice PGPORT=5432 node_modules/lambda-local/bin/lambda-local -l dist/index.js -h handler -e test/gettile.js",
    "predeploy": "tsc && zip -jr ./dist/function.zip ./dist/index.js && zip -qr ./dist/function.zip ./node_modules",
    "deploy": "aws lambda update-function-code --function-name tileserver --zip-file fileb://dist/function.zip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henrythasler/aws-tileserver.git"
  },
  "keywords": [
    "vectortiles",
    "postgis",
    "lambda",
    "mapbox",
    "mvt",
    "terraform",
    "aws"
  ],
  "author": "Henry Thasler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/henrythasler/aws-tileserver/issues"
  },
  "homepage": "https://github.com/henrythasler/aws-tileserver#readme",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.27",
    "@types/node": "^12.0.12",
    "@types/pg": "^7.4.14",
    "lambda-local": "^1.6.3"
  },
  "dependencies": {
    "pg": "^7.11.0"
  }
}
