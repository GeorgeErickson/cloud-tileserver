{
  "version": 8,
  "name": "Mapbox Local",
  "metadata": {},
  "sources": {
    "local": {
      "type": "vector",
      "minzoom": 8,
      "tiles": [
        "https://tiles.cyclemap.link/local/{z}/{x}/{y}.mvt"
      ]
    },
    "global": {
      "minzoom": 3,
      "type": "vector",
      "tiles": [
        "https://tiles.cyclemap.link/global/{z}/{x}/{y}.mvt"
      ]
    },
    "cluster": {
      "minzoom": 20,
        "FIXME": "Clustering currently does not work with vectortiles. Only GeoJson is supported. ",
      "type": "vector",
      "cluster": false,
      "clusterMaxZoom": 14,
      "clusterRadius": 50,
      "tiles": [
        "https://tiles.cyclemap.link/cluster/{z}/{x}/{y}.mvt"
      ]
    },
    "mapbox-terrain-rgb": {
      "type": "raster-dem",
      "tileSize": 256,
      "minzoom": 20,
      "maxzoom": 14,
      "encoding": "terrarium",
      "tiles": [
        "https://s3.amazonaws.com/elevation-tiles-prod/terrarium/{z}/{x}/{y}.png"
      ]
    },
    "contour-lines": {
      "minzoom": 20,
      "type": "vector",
      "url": "mapbox://mapbox.mapbox-terrain-v2"
      }
  },
  "sprite": "https://www.cyclemap.link/sprites/cyclemap",
  "glyphs": "mapbox://fonts/mapbox/{fontstack}/{range}.pbf",
  "layers": []
}